<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <managed-bean>
        <managed-bean-name>countryNavigator</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.CountryNavigator</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>countries</property-name>
            <list-entries>
                <value>#{egypt}</value>
                <value-class>com.jsfprohtml5.countrynavigator.model.Country</value-class>
                <value>#{germany}</value>
                <value-class>com.jsfprohtml5.countrynavigator.model.Country</value-class>
                <value>#{brazil}</value>                                
                <value-class>com.jsfprohtml5.countrynavigator.model.Country</value-class>
            </list-entries>
        </managed-property>
    </managed-bean>
    
    <!-- Egypt -->    
    <managed-bean>
        <managed-bean-name>egypt</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.Country</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Egypt</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>82000000</value>
        </managed-property>  
        <managed-property>
            <property-name>cities</property-name>
            <list-entries>
                <value>#{cairo}</value>
                <value-class>com.jsfprohtml5.countrynavigator.model.City</value-class>
                <value>#{alexandria}</value>
                <value-class>com.jsfprohtml5.countrynavigator.model.City</value-class>
                <value>#{aswan}</value>                                
                <value-class>com.jsfprohtml5.countrynavigator.model.City</value-class>
            </list-entries>
        </managed-property>             
    </managed-bean>  
    <managed-bean>
        <managed-bean-name>cairo</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.City</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Cairo</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>8000000</value>
        </managed-property>       
    </managed-bean>            
    <managed-bean>
        <managed-bean-name>alexandria</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.City</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Alexandria</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>5000000</value>
        </managed-property>       
    </managed-bean>          
    <managed-bean>
        <managed-bean-name>aswan</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.City</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Aswan</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>280000</value>
        </managed-property>       
    </managed-bean>         

    <!-- Germany -->
    <managed-bean>
        <managed-bean-name>germany</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.Country</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Germany</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>81000000</value>
        </managed-property> 
        <managed-property>
            <property-name>cities</property-name>
            <list-entries>
                <value>#{berlin}</value>
                <value-class>com.jsfprohtml5.countrynavigator.model.City</value-class>
                <value>#{hamburg}</value>
                <value-class>com.jsfprohtml5.countrynavigator.model.City</value-class>
                <value>#{munich}</value>                                
                <value-class>com.jsfprohtml5.countrynavigator.model.City</value-class>
            </list-entries>
        </managed-property>             
    </managed-bean>    
    <managed-bean>
        <managed-bean-name>berlin</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.City</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Berlin</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>3500000</value>
        </managed-property>       
    </managed-bean>            
    <managed-bean>
        <managed-bean-name>hamburg</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.City</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Hamburg</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>1800000</value>
        </managed-property>       
    </managed-bean>          
    <managed-bean>
        <managed-bean-name>munich</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.City</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Munich</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>1400000</value>
        </managed-property>       
    </managed-bean>     
    
    <!-- Brazil -->
    <managed-bean>
        <managed-bean-name>brazil</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.Country</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Brazil</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>196000000</value>
        </managed-property>   
        <managed-property>
            <property-name>cities</property-name>
            <list-entries>
                <value>#{saoPaulo}</value>
                <value-class>com.jsfprohtml5.countrynavigator.model.City</value-class>
                <value>#{rioDeJaneiro}</value>
                <value-class>com.jsfprohtml5.countrynavigator.model.City</value-class>
                <value>#{brasilia}</value>                                
                <value-class>com.jsfprohtml5.countrynavigator.model.City</value-class>
            </list-entries>
        </managed-property>              
    </managed-bean>   
    <managed-bean>
        <managed-bean-name>saoPaulo</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.City</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Sao Paulo</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>20000000</value>
        </managed-property>       
    </managed-bean>            
    <managed-bean>
        <managed-bean-name>rioDeJaneiro</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.City</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Rio de Janeiro</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>12000000</value>
        </managed-property>       
    </managed-bean>          
    <managed-bean>
        <managed-bean-name>brasilia</managed-bean-name>
        <managed-bean-class>com.jsfprohtml5.countrynavigator.model.City</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
        <managed-property>
            <property-name>name</property-name>
            <value>Brasilia</value>
        </managed-property>
         <managed-property>
            <property-name>population</property-name>
            <value>4000000</value>
        </managed-property>       
    </managed-bean>         
</faces-config>
